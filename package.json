{
	"name": "ts-ssr-kit",
	"version": "1.0.0",
	"engines": {
		"node": "17.x"
	},
	"main": "./build/bundle.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:build-client\" \"npm run dev:tailwind\"",
		"dev:server": "nodemon --watch ./src/** -e ts,json,tsx --exec \"npx webpack --config webpack.server.js --mode development && node build/bundle.js\" --MODE=dev",
		"dev:build-client": "webpack --config webpack.client.js --mode development --watch",
		"dev:tailwind": "npx tailwindcss -o public/tailwind.css --watch",
		"start": "node build/bundle.js",
		"build": "webpack --config webpack.client.js --mode production && webpack --config webpack.server.js --mode production && npx tailwindcss -o public/tailwind.css --minify",
		"build:start": "npm run build && npm start"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@tailwindcss/forms": "^0.3.4",
		"@tailwindcss/typography": "^0.4.1",
		"axios": "^0.24.0",
		"compression": "^1.7.4",
		"d3": "^7.1.1",
		"express": "^4.17.1",
		"graphql-ws": "^5.5.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-router-dom": "^6.0.2",
		"serialize-javascript": "^6.0.0",
		"tailwindcss": "^2.2.19"
	},
	"devDependencies": {
		"@swc/core": "^1.2.108",
		"@types/compression": "^1.7.2",
		"@types/d3": "^7.1.0",
		"@types/express": "^4.17.13",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"@types/react-helmet": "^6.1.4",
		"@types/react-router-dom": "^5.3.2",
		"@types/serialize-javascript": "^5.0.1",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"concurrently": "^6.4.0",
		"css-loader": "^6.5.1",
		"esbuild-loader": "^2.16.0",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.1",
		"eslint-plugin-react": "^7.27.1",
		"mini-css-extract-plugin": "^2.4.4",
		"nodemon": "^2.0.9",
		"postcss-loader": "^6.2.0",
		"sass": "^1.43.4",
		"sass-loader": "^12.3.0",
		"source-map-loader": "^3.0.0",
		"style-loader": "^3.3.1",
		"swc-loader": "^0.1.15",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4",
		"webpack": "^5.64.0",
		"webpack-cli": "^4.9.1",
		"webpack-node-externals": "^3.0.0"
	},
	"description": "",
	"browserslist": [
		"last 2 versions",
		"Android >= 10.0",
		"ios >= 14"
	]
}
